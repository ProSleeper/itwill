package com.day6;

import java.util.Scanner;

public class Test2 {

	public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int numOfTry = sc.nextInt();
        
        int zero = 0;
        int one = 1;
        
        System.out.println(fibonacci(numOfTry - 2, zero, one));
        
    }
    
    public static int fibonacci(int tryNumber, int lNum, int rNum){
        
    
    	
        if(tryNumber < 1){
            return lNum + rNum;
        }
        return fibonacci(--tryNumber, rNum, lNum + rNum);
    }

}
