package com.day3;

import java.util.Scanner;

class DataInt{
	int num = 0;
}

public class Test7 {
	static void SwapNumber(DataInt lNum, DataInt rNum) {
		int temp = 0;
		temp = lNum.num;
		lNum.num = rNum.num;
		rNum.num = temp;
	}
	
	public static void main(String[] args) {
		
		//3개의 수를 입력받아 작은 수에서 큰수 순으로 정렬
		
		
		Scanner sc = new Scanner(System.in);
		
		DataInt num1, num2, num3, temp;
		
		System.out.println("숫자 3개를 입력하세요");
		
		num1.num = sc.nextInt();
		num2.num = sc.nextInt();
		num3.num = sc.nextInt();
		
		if(num1.num > num2.num) {
			SwapNumber(num1, num2);
		}
		
		if(num2.num > num3.num) {
			SwapNumber(num2, num3);
		}
		
		if(num1.num > num2.num) {
			SwapNumber(num1, num2);
		}
		
		System.out.println(num1.num + " " + num2.num + " " + num3.num);
		
	}
	
}
