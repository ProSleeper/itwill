package com.day3;

import java.util.Scanner;

class DataInt{
	
	int num;
	DataInt(){
		num = 0;
	}
}

public class Test7 {
	static void SwapNumber(int[] arr, Integer lNum, Integer rNum) {
		Integer temp;
		if(lNum > rNum) {
			temp = lNum;
			lNum = rNum;
			rNum = temp;
		}
		
	}
	
	public static void main(String[] args) {
		
		//3개의 수를 입력받아 작은 수에서 큰수 순으로 정렬
		
		
		Scanner sc = new Scanner(System.in);
		
		int[] arr = new int[4];
		
		System.out.println("숫자 3개를 입력하세요");
		
		arr[0] = sc.nextInt();
		arr[1] = sc.nextInt();
		arr[2] = sc.nextInt();
		
		SwapNumber(arr, arr[0], arr[1]);
		SwapNumber(arr, arr[1], arr[2]);
		SwapNumber(arr, arr[0], arr[1]);
		
		
		System.out.println(arr[0] + " " + arr[1] + " " + arr[2]);
		
	}
	
}
