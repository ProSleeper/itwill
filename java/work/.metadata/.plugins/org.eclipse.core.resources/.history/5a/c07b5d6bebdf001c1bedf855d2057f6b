package com.day3;

import java.util.Scanner;

class DataInt{
	
	int num;
	DataInt(){
		num = 0;
	}
}

public class Test7 {
	static void SwapNumber(Integer lNum, Integer rNum) {
		Integer temp;
		if(lNum > rNum) {
			temp = lNum;
			lNum = rNum;
			rNum = temp;
		}
		
	}
	
	public static void main(String[] args) {
		
		//3개의 수를 입력받아 작은 수에서 큰수 순으로 정렬
		
		
		Scanner sc = new Scanner(System.in);
		
		Integer num1 = new Integer(0);
        Integer num2 = new Integer(0);
        Integer num3 = new Integer(0);
        Integer temp = new Integer(0);
		
		System.out.println("숫자 3개를 입력하세요");
		
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		num3 = sc.nextInt();
		
		if(num1 > num2) {
			SwapNumber(num1, num2);
		}
		
		if(num2 > num3) {
			SwapNumber(num2, num3);
		}
		
		if(num1 > num2) {
			SwapNumber(num1, num2);
		}
		
		System.out.println(num1 + " " + num2 + " " + num3);
		
	}
	
}
